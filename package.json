{
  "name": "hd-calc",
  "version": "1.0.0",
  "description": "Human Design Calculator Web Service",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postinstall": "npm run build"
  },
  "keywords": [
    "human-design",
    "calculator",
    "api",
    "typescript",
    "express"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "typescript": "^5.0.0",
    "@types/node": "^18.0.0",
    "@types/express": "^4.17.17",
    "redis": "^4.6.7",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "@types/redis": "^4.0.11"
  }
}
